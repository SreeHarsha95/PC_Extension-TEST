{
    "manifest_version" : 5,
    "name" : "PC Data Insert",
    "version" : "0.1",
    "externally_connectable": {
        "matches": ["http://localhost:8080/"]
    },
    "content_scripts" : [
        {
        "matches" : ["<all_urls>"],
        "js":["popup.html"]
}],
    "background":{
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions" :[
        "storage"
    ],

    "browser_action":{
        "default_icon" : "ct.png",
        "default_popup" : "popup.html",
        "default_title" : "Lets insert a word"
    }

}